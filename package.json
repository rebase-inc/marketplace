{
  "name": "alveare-react",
  "version": "0.0.1",
  "description": "Web app for Alveare",
  "repository": "https://github.com/alveare/app",
  "main": "src/app.js",
  "dependencies": {
    "flux": "^2.0.1",
    "react": "^0.12.0",
    "keymirror": "~0.1.0"
  },
  "devDependencies": {
    "browserify": "^6.2.0",
    "reactify": "^0.15.2",
