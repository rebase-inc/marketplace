  "scripts": {
    "start": "watchify -o build/bundle.js -v -d src/app.js",
    "build": "browserify . > build/bundle.js",
    "test": "jest"
  },
  "author": "Bill Fisher",
  "browserify": {
    "extension": ["jsx"],
    "transform": ["reactify"]
  }
}
